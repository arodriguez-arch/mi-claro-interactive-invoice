{
  "name": "@e4gs/mi-claro-interactive-invoice",
  "version": "0.4.13",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/mi-claro-interactive-invoice/mi-claro-interactive-invoice.esm.js",
  "exports": {
    ".": {
      "import": "./dist/mi-claro-interactive-invoice/mi-claro-interactive-invoice.esm.js",
      "require": "./dist/mi-claro-interactive-invoice/mi-claro-interactive-invoice.cjs.js"
    },
    "./mi-claro-interactive-invoice": {
      "import": "./dist/components/mi-claro-interactive-invoice.js",
      "types": "./dist/components/mi-claro-interactive-invoice.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "files": [
    "dist/",
    "loader/",
    "scripts/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve --port=4200",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "npm-publish": "npm publish --access restricted",
    "copy-assets": "node scripts/copy-assets.js"
  },
  "bin": {
    "intellisoft-mi-claro-interactive-invoice-copy-assets": "./scripts/copy-assets.js"
  },
  "devDependencies": {
    "@stencil/angular-output-target": "^1.1.1",
    "@stencil/core": "^4.37.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^24.3.0"
  },
  "license": "MIT",
  "dependencies": {
    "tippy.js": "^6.3.7"
  }
}
